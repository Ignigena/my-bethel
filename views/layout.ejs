<!DOCTYPE html>
<html lang="en" data-ng-app="Bethel" data-ng-controller="AppCtrl" class="no-js">
<head>
  <meta charset="utf-8" />
  <!--[if IE]><meta http-equiv="X-UA-Compatible" content="IE=edge" /><![endif]-->
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Bethel</title>
  <!--STYLES-->
  <link rel="stylesheet" href="min/production.min.css">
  <!--STYLES END-->
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=RobotoDraft:300,400,500,700,400italic">
  <link rel="stylesheet" href="//s3.amazonaws.com/static.bethel.io/styles/rickshaw.min.css">
  <link rel="stylesheet" href="//s3.amazonaws.com/static.bethel.io/styles/medium-editor/medium-editor.min.css">
  <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
    ga('create', 'UA-45872811-1', { 'cookieDomain': 'none' });
  </script>
</head>

<body layout="row" data-ng-class="{ 'nav-collapsed': collapseNav }">
  <md-sidenav class="md-sidenav-left md-whiteframe-z2" md-component-id="left" md-is-locked-open="$mdMedia('gt-md')">
    <md-toolbar class="md-hue-2">
      <h1 class="md-toolbar-tools">Bethel</h1>
    </md-toolbar>

    <md-list class="md-accent md-hue-3">
      <md-list-item ng-repeat="link in navLinks" ng-click="nav(link.url)" ng-cloak>
        <md-icon md-svg-icon="icons/ic_{{ link.icon }}_24px.svg"></md-icon>
        <p>{{ link.title }}</p>
      </md-list-item>
    </md-list>
  </md-sidenav>

  <section layout="column" class="main-content" flex>
    <%- body %>
  </section>

  <!--TEMPLATES-->
  
  <!--TEMPLATES END-->

  <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.3.5/angular.min.js"></script>
  <script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>

  <!--SCRIPTS-->
  <script src="min/production.min.js"></script>
  <script src="concat/dependencies.js"></script>
  <!--SCRIPTS END-->

  <script>
    videojs.options.flash.swf = "https://static.bethel.io/libraries/video-js/video-js.swf";
  </script>

  <!--LIBRARIES-->
  <script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.10.4/jquery-ui.min.js"></script> 
  <script src="//s3.amazonaws.com/static.bethel.io/js/bootstrap-datepicker.js"></script>
  <script src="//s3.amazonaws.com/static.bethel.io/js/jquery.form.js"></script>
  <script src="//s3.amazonaws.com/static.bethel.io/js/jquery.typeahead.js"></script>
  <script src="//s3.amazonaws.com/static.bethel.io/js/jquery.validate.js"></script>
  <!--LIBRARIES END-->
  </body>

</html>
