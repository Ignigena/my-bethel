<body data-ng-class="{ 'nav-collapsed': collapseNav }">
  <nav class="navbar navbar-inverse" role="navigation">
    <div class="container-fluid">
      <div class="navbar-header">
        <a class="navbar-brand" href="/">Bethel</a>
      </div>
      <ul class="nav navbar-nav">
        <li><a data-ng-click="toggleNav()"><i class="fa fa-bars"></i></a></li>
      </ul>
      <ul class="nav navbar-nav navbar-right" ng-show="user.name">
        <li class="dropdown" data-ng-show="ministry.name">
          <a href="" class="dropdown-toggle ministry" data-toggle="dropdown" data-ng-bind="ministry.name"></a>
          <ul class="dropdown-menu">
            <li ng-repeat="link in ministryLinks"><a data-ng-href="{{ link.url }}" data-ng-bind="link.title"></a></li>
          </ul>
        </li>
        <li class="dropdown"><a class="avatar"><img class="avatar" data-ng-src="{{ user.avatar }}" alt="{{ user.name }}" /></a></li>
        <li><a href="logout" title="Log out"><i class="fa fa-sign-out"></i></a></li>
      </ul>
    </div>
  </nav>

  <div class="ui viewport primary">
    <div class="ui horizontal">

      <nav class="ui column navigation" role="navigation">
        <ul id="main-menu" class="nav nav-pills nav-stacked nav-main" ngCloak>
          <li ng-repeat="link in navLinks"><a href="{{ link.url }}"><i class="fa fa-{{ link.icon }}"></i>{{ link.title }}</a></li>
        </ul>
      </nav>

      <section class="ui column page-wrap">
        <%- body %>
      </section>

    </div>
  </div>

