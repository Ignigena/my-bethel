<!DOCTYPE html>
<html lang="en" ng-app="Bethel">
<head>
  <meta charset="utf-8" />
  <!--[if IE]><meta http-equiv="X-UA-Compatible" content="IE=edge" /><![endif]-->
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Bethel</title>
  <!--STYLES-->
  <link rel="stylesheet" href="/concat/production.css">
  <!--STYLES END-->
  <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
    ga('create', 'UA-45872811-1', { 'cookieDomain': 'none' });
  </script>
</head>

<body layout="column" ng-controller="AppCtrl">

  <div id="outdated">
    <h6>Your browser is out of date!</h6>
    <p>You'll need a more recent browser to use Bethel. Click below for a few suggestions...
      <a id="btnUpdateBrowser" href="http://outdatedbrowser.com/">Update my browser now </a></p>
    <p class="last"><a href="#" id="btnCloseUpdateBrowser" title="Close">&times;</a></p>
  </div>

  <top-nav ministry="ministry" ministries="authorized" ng-if="user" title="pageTitle" user="user"></top-nav>

  <section layout="row" layout-fill flex>

    <md-sidenav class="md-sidenav-left md-whiteframe-z2" layout="column" md-component-id="left" md-is-locked-open="$mdMedia('gt-xs')" ng-cloak ng-if="user">
      <md-content flex>
        <md-list class="md-accent md-hue-3">
          <md-list-item ng-repeat="link in navLinks" ng-click="nav(link.url)" ng-class="{ 'active': navActive == link.url }" ng-cloak>
            <md-icon>{{ link.icon }}</md-icon>
            <p>{{ link.title }}</p>
          </md-list-item>
        </md-list>
      </md-content>
    </md-sidenav>

    <div class="main-content" flex layout="column" ng-if="user" ui-view>
      <!--ANGULAR VIEW-->
    </div>

  </section>

  ${ body }

  <script src="/socket.io/socket.io.js"></script>
  <script src="/libraries/sails.io.js" autoConnect="false"></script>
  <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.0/angular.min.js"></script>
  <script src="/libraries/angular.min.js"></script>
  <script src="/libraries/video.min.js"></script>
  <!--SCRIPTS-->
  <script src="/concat/production.js"></script>
  <!--SCRIPTS END-->

  <script>
    function browserCheck(a){var b=window.onload;window.onload='function'!=typeof window.onload?a:function(){b&&b();a()}}
    browserCheck(function(){outdatedBrowser({bgColor:'#f25648',color:'#fff',lowerThan:'flex',languagePath:''})});
  </script>

</body>

</html>
