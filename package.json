    "test": "npm run unit && npm run protractor",
    "posttest": "cat coverage/lcov.info | ./node_modules/.bin/codacy-coverage --token 411a15d5ec144bcc89cd4f57b147bb8f --verbose",
    "mocha": "mocha test/sails.spec.js test/sails/*.js --timeout 15000",
    "preprotractor": "./node_modules/.bin/webdriver-manager update",
    "protractor": "protractor test/e2e.config.js",
    "karma": "karma start test/angular.config.js",
    "unit": "istanbul cover -x '**/config/**' -x 'api/responses/**' --dir ./coverage/sails _mocha -- test/sails.spec.js test/sails/*.js --timeout 15000 && npm run karma",
